{
  "devCommand": "uvicorn api/chat/main:app --host 0.0.0.0 --port 8000",
  "builds": [
    {
      "src": "api/chat/*.py",
      "use": "@vercel/python"
    },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/api/chat/(.*)",
      "dest": "api/chat/main.py"
    }
  ]
}
